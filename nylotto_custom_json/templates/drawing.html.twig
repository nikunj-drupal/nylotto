{#
/**
 * @file drawing.html.twig
 * Default theme implementation to present Drawing data.
 *
 * This template is used when viewing Drawing pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_drawing()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('drawing') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
